{
    "name": "kayw-geek/changelog-cli",
    "description": "Subscript some changelog of library on CLI",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "KaywGeek\\ChangelogCli\\": "cli/"
        }
    },
    "authors": [
        {
            "name": "Kai Wei",
            "email": "weikaiii@sina.cn"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "scripts": {
        "pint": "vendor/bin/pint -c pint.json",
        "phpstan": "vendor/bin/phpstan analyse",
        "baseline": "vendor/bin/phpstan --generate-baseline",
        "build": "php  app:build changelog"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.4",
        "laravel-zero/framework": "^9.1",
        "laravel/pint": "^0.2.3",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/termwind": "^1.13",
        "phpstan/phpstan": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/logpull"]
}
